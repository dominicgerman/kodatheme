{
  "$schema": "https://schemas.wp.org/wp/6.2/theme.json",
  "version": 2,
  "settings": {
    "color": {
      "palette": [
        {
          "name": "Dark",
          "slug": "dark",
          "color": "#1B2233"
        },
        {
          "name": "Light",
          "slug": "light",
          "color": "#517398"
        }
      ]
    },
    "spacing": {
      "margin": true,
      "padding": true,
      "blockGap": false,
      "units": ["px", "em", "rem", "vh", "vw"]
    },
    "typography": {
      "fontFamilies": [
        {
          "fontFamily": "'Raleway', sans-serif",
          "slug": "raleway",
          "name": "Raleway",
          "fontFace": [
            {
              "fontFamily": "'Raleway', sans-serif",
              "fontWeight": "200",
              "src": ["file:./assets/fonts/Raleway-ExtraLight.ttf"]
            },
            {
              "fontFamily": "'Raleway', sans-serif",
              "fontWeight": "400",
              "src": ["file:./assets/fonts/Raleway-Regular.ttf"]
            },
            {
              "fontFamily": "'Raleway', sans-serif",
              "fontWeight": "600",
              "src": ["file:./assets/fonts/Raleway-SemiBold.ttf"]
            }
          ]
        }
      ]
    },
    "blocks": {
      "core/group": {
        "color": {
          "palette": [
            {
              "name": "Dark",
              "slug": "dark",
              "color": "#1B2233"
            },
            {
              "name": "Light",
              "slug": "light",
              "color": "#517398"
            }
          ]
        }
      }
    }
  },
  "styles": {
    "elements": {
      "link": {
        "typography": {
          "textDecoration": "none"
        }
      }
    },
    "typography": {
      "fontFamily": "var(--wp--preset--font-family--raleway)",
      "fontSize": "1rem"
    }
  },
  "templateParts": [
    {
      "area": "header",
      "name": "header",
      "title": "Header"
    },
    {
      "area": "footer",
      "name": "footer",
      "title": "Footer"
    }
  ]
}
